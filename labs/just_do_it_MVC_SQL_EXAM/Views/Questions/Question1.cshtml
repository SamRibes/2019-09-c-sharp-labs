@model IEnumerable<just_do_it_MVC_SQL_EXAM.Customer>

@{
    ViewData["Title"] = "Question 1";
}

<h2>Question 1</h2>

<table class="table table-striped table-hover table-responsive">
    <thead>
        <tr>
            <th>
                Contact Name
            </th>
            <th>
                City
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in ViewBag.customers)
        {
            <tr>
                <td>
                    @item.ContactName
                </td>
                <td>
                    @item.City
                </td>
            </tr>
        }
    </tbody>
</table>
